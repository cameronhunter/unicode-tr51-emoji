{
  "name": "unicode-tr51-emoji",
  "version": "2.0.1",
  "license": "MIT",
  "author": "Cameron Hunter <hello@cameronhunter.co.uk>",
  "description": "Emoji data parsed from Unicode Technical Report #51 v1.0 â€“ v4.0",
  "main": "index.js",
  "scripts": {
    "rollup:help": "rollup --help",
    "compile:emoji-data": "rollup -c --input ./src/emoji-data.pegjs --output ./parser/emoji-data.js",
    "compile:emoji-sequences": "rollup -c --input ./src/emoji-sequences.pegjs --output ./parser/emoji-sequences.js",
    "compile": "npm run compile:emoji-data && npm run compile:emoji-sequences",
    "prebuild": "npm run compile",
    "build:v1.0": "bin/build -i ./data/v1.0 -o ./v1.0",
    "build:v2.0": "bin/build -i ./data/v2.0 -o ./v2.0",
    "build:v3.0": "bin/build -i ./data/v3.0 -o ./v3.0",
    "build:v4.0": "bin/build -i ./data/v4.0 -o ./v4.0",
    "build:latest": "bin/build -i ./data/v4.0 -o .",
    "build": "npm run build:v1.0 && npm run build:v2.0 && npm run build:v3.0 && npm run build:v4.0 && npm run build:latest",
    "pretest": "npm run build",
    "test": "ava --tap | tap-diff",
    "prepublish": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cameronhunter/unicode-tr51-emoji.git"
  },
  "bugs": "https://github.com/cameronhunter/unicode-tr51-emoji/issues",
  "keywords": [
    "unicode",
    "unicode-data",
    "code points",
    "symbols",
    "characters",
    "emoji",
    "data"
  ],
  "files": [
    "v1.0",
    "v2.0",
    "v3.0",
    "v4.0",
    "emoji-data.js",
    "emoji-sequences.js",
    "emoji-zwj-sequences.js",
    "index.js",
    "code-points.js",
    "symbols.js"
  ],
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-preset-es2015-rollup": "^1.2.0",
    "babel-preset-stage-1": "^6.16.0",
    "jsesc": "^2.2.0",
    "mkdirp": "^0.5.1",
    "nomnom": "^1.8.1",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-pegjs": "^2.1.1",
    "rollup": "^0.38.0",
    "tap-diff": "^0.1.1"
  }
}
